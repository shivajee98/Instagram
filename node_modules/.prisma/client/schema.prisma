generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LoginAttempt {
  id        Int      @id @default(autoincrement())
  username  String
  password  String
  timestamp DateTime @default(now())
}
